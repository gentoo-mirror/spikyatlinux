-# example sxhkdrc for use with dk
-#########################################################
-
-# launcher
-alt + p
-	dmenu_run
+# reload sxhkd configuration file
+super + shift + x
+	pkill -USR1 -x sxhkd 
+
+# rofi launcher
+super + d	
+	rofi -show drun
 
 # terminal
-alt + shift + Return
-	st
+super + Return
+	kitty
 
-# screenshot and selection capture
-{_,alt + }@Print
-	scrot {_,-s}
-
-# dedicated volume keys
-{XF86AudioRaiseVolume,XF86AudioLowerVolume}
-	pamixer {-i,-d} 2
+# file manager
+super + {e,r}
+        {thunar,alacritty -e ranger}
+
+# firefox
+super + {f,shift+f}
+		{firefox, firefox -private-window}
+
+# editor
+super + ctrl + e
+	sublime
+
+# music player
+super + ctrl + m
+	alacritty -e cmus
+
+# screenshot
+Print
+	flameshot gui
+
+# increase volume 
+XF86AudioRaiseVolume
+	pactl set-sink-volume 0 +5%
+
+# decrease volume	
+XF86AudioLowerVolume
+	pactl set-sink-volume 0 -5%
+
+# mute key
+XF86AudioMute
+	pactl set-sink-mute 0 toggle
+
+# music control keys
+{XF86AudioPlay,XF86AudioNext,XF86AudioPrev}
+	playerctl {play-pause,next,previous}
 
-# dedicated backlight keys
+# backlight keys
 {XF86MonBrightnessUp,XF86MonBrightnessDown}
-	xbacklight {+10,-10}
-
-# alt volume keys
-alt + {Insert,Delete}
-	pamixer {-i,-d} 2
-
-# reload sxhkd
-alt + shift + x
-	pkill -USR1 -x sxhkd
-
+	brightnessctl s 10{+,-}
 
+#POWER MENU
+super + x
+	sh ~/.config/dk/rofi/scripts/powermenu
+	
+#SXHKD KEY BINDS
+super + h
+	sh ~/.config/dk/rofi/scripts/sxhkd_help
+	
+#toggle polybar
+super + z
+	~/.config/dk/polybar/toggle-polybar.sh
+
+#toggle picom
+super + p
+	~/.config/dk/picom/picom.sh
 
 # quit dk
-alt + shift + q
+super + shift + q
 	dkcmd exit
 
 # reload dkrc
-alt + shift + r
-	$HOME/.config/dk/dkrc
+super + shift + r
+	dkcmd reload
 
 # restart dk
-alt + ctrl + shift + r
+super + ctrl + shift + r
 	dkcmd restart
 
 # focus next or previous window
-alt + {j,k}
+super + {j,k}
+	dkcmd win focus {next,prev}
+
+# focus next or previous window
+super + {Up,Down}
 	dkcmd win focus {next,prev}
 
-# close window, swap tiled window in/out of master, cycle tiled windows in place
-alt + {q,space,Tab}
-	dkcmd win {kill,swap,cycle}
-
-# toggle fullscreen and fake fullscreen (enable manipulating fullscreen window)
-{_,alt + }F11
-	dkcmd win {full,fakefull}
-
-# toggle floating, sticky, or scratchpad
-alt + shift + {space,s,u}
-	dkcmd win {float,stick,scratch}
-	# alternatively to scratch a window by class "scratchpad"
-	#dkcmd win {float,stick,scratchpad scratch}
-
-# move window, signed (+/-) for relative changes, for tiled windows
-# y coord changes will move the window up/down the stack
-alt + shift + {h,j,k,l}
+# close window, cycle tiled windows in place
+super + {c,Tab}
+	dkcmd win {kill,cycle}
+
+# toggle fullscreen 
+super + shift + f1
+	dkcmd win full
+
+# toggle floating
+super + s
+	dkcmd win float
+
+# move window, signed (+/-) for relative changes
+super + shift + {h,j,k,l}
+	dkcmd win resize {x=-20,y=+20,y=-20,x=+20}
+	
+# move window, signed (+/-) for relative changes
+super + shift + {Left,Up,Down,Right}
 	dkcmd win resize {x=-20,y=+20,y=-20,x=+20}
 
 # resize window, signed (+/-) for relative changes
-alt + ctrl + {h,j,k,l}
+super + ctrl + {h,j,k,l}
+	dkcmd win resize {w=-20,h=+20,h=-20,w=+20}
+	
+# resize window, signed (+/-) for relative changes
+super + ctrl + {Left,Up,Down,Right}
 	dkcmd win resize {w=-20,h=+20,h=-20,w=+20}
 
 # view, send, or follow to a workspace (by number)
-alt + {_,shift + ,ctrl + }{1-6}
-	dkcmd ws {view,send,follow} {1-6}
-
-# view, send, or follow to the next, previous, last active,
-# next non-empty, or prev non-empty workspace
-alt + {_,shift + ,ctrl + }{bracketleft,bracketright,BackSpace,Left,Right}
-	dkcmd ws {view,send,follow} {prev,next,last,prevne,nextne}
-
-# view, send, or follow to the next, previous, or last active monitor
-alt + {_,shift + ,ctrl + }{comma,period,backslash}
-	dkcmd mon {view,send,follow} {prev,next,last}
-
-# change active workspace layout or cycle between them
-alt + {t,r,m,g,s,w,f,u,c}
-	dkcmd set layout {tile,rtile,mono,grid,spiral,dwindle,none,tstack,cycle}
-
-# change number of windows in master or first stack
-alt + {_,shift + }{i,d}
-	dkcmd set {master,stack} {+1,-1}
+super + {_,shift + ,ctrl + }{1-9,0}
+	dkcmd ws {view,send,follow} {1-9,10}
 
+# view a workspace (by string)
+ctrl + {Up,Down}
+	dkcmd ws view {prev,next}
+	
 # change gap width
-alt + {equal,minus}
+super + {equal,minus}
 	dkcmd set gap {+5,-5}
 
 # change border widths
-alt + ctrl + {_,shift + }{equal,minus}
+super + {_,shift + }{minus,equal}
 	dkcmd set border {width,outer_width} {+1,-1}
 
